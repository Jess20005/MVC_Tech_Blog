<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<a href="/edit-posts/{{post.id}}" class="card mb-2" style="text-decoration: none; color: black;">
    <div class="card-header">
        <span class="d-flex justify-content-between">
            <span>{{post.title}}</span>
            <span>{{format_date post.createdAt}}--{{format_date post.createdAt}}</span>
        </span>
    </div>
</a>
{{/each}}